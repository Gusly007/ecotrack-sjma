/**
 * Fichier .env.example - Copier en .env et adapter les valeurs
 */

# Port du serveur
PORT=3000

# Environnement
NODE_ENV=development

# PostgreSQL
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=your_password_here
PGDATABASE=ecotrack-db

# Logging
LOG_LEVEL=debug

# API
API_VERSION=1.0.0
API_BASE_URL=http://localhost:8080/api
